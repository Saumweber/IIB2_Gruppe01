<?xml version="1.0" ?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="auftrag">
    <xs:complexType>
      <xs:sequence>
        <xs:element type="xs:string" name="beschreibung" minOccurs="1" maxOccurs="1"/>
      </xs:sequence>
      <xs:attribute type="xs:unsignedInt" name="id" use="required"/>
      <xs:attribute type="xs:unsignedLong" name="gebaeude" use="required"/>
      <xs:attribute type="xs:string" name="status" use="required"/>
    </xs:complexType>
  </xs:element>
</xs:schema>